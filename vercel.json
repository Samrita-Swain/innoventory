{
  "buildCommand": "npx prisma generate && npx next build",
  "installCommand": "npm ci",
  "env": {
    "PRISMA_GENERATE_DATAPROXY": "true",
    "SKIP_ENV_VALIDATION": "true",
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    },
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "framework": "nextjs",
  "outputDirectory": ".next"
}